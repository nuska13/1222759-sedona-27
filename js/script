var btn = document.querySelector(".modal-form-button");
var form = document.querySelector(".modal-form");
btn.addEventListener("click", function (evt) {
evt.preventDefault();
if (modal-form.classList.contains("modal-form-visible")) {
modal-form.classList.add("modal-form-invisible")
modal-form.classList.remove("modal-form-visible")
}
else {
modal-form.classList.add("modal-form-visible");
modal-form.classList.remove("modal-form-invisible");
});
